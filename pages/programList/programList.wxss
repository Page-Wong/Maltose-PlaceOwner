/* pages/programReview/programReview.wxss */
@import "/miniprogram_npm/vant-weapp/card/index.wxss";

.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  color: #aaa;
}


.result-item{
  position: relative;
  display: flex;
  flex-direction: column;
  padding: 20rpx 0 20rpx 0;
  overflow: hidden;
  border-bottom: 2rpx solid #e5e5e5;
}
 

.loading{
  padding: 10rpx;
  text-align: center;
}

.auditModalBtn{
  padding: 50rpx;
  width: 300px;
}
